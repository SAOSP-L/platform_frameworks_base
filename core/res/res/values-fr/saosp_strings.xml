<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- kill app long press back -->
    <string name="app_killed_message">Application arrêtée.</string>

    <!-- Title of dialog to confirm rebooting. -->
    <string name="reboot_title">Redémarrer</string>

    <!-- Reboot Confirmation Dialog.  When the user chooses to reboot the device, there will
         be a confirmation dialog.  This is the message. -->
    <string name="reboot_confirm" product="tablet">Votre tablette redémarre.</string>
    <string name="reboot_confirm" product="default">Votre téléphone redémarre.</string>

    <!-- label for item that reboots the phone in phone options dialog -->
    <string name="global_action_reboot">Redémarrer</string>

    <!-- label for current user in phone options dialog -->
    <string name="global_action_current_user">Actuel</string>

    <!-- label for item that screenshots in phone options dialog -->
    <string name="global_action_screenshot">Capture d\'écran</string>

    <!-- Reboot Progress Dialog. This is shown if the user chooses to reboot the phone. -->
    <string name="reboot_progress">Redémarrage\u2026</string>

    <!-- Reboot menu -->
    <!-- Button to reboot the phone, within the Reboot Options dialog -->
    <string name="reboot_reboot">Redémarrer</string>
    <!-- Button to reboot the phone into recovery, within the Reboot Options dialog -->
    <string name="reboot_recovery">Recovery</string>
    <!-- Button to reboot the phone into bootloader, within the Reboot Options dialog -->
    <string name="reboot_bootloader">Bootloader</string>
    <!-- Button to soft reboot the device, within the Reboot Options dialog -->
    <string name="reboot_soft">Redémarrage logiciel</string>

    <!-- Title of a category of application permissions, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permgrouplab_security">Sécurité</string>
    <!-- Description of a category of application permissions, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permgroupdesc_security">Permissions relatives à l\'information de sécurité de l\'appareil.</string>

    <!-- [CHAR LIMIT=NONE] Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_readPhoneBlacklist">Lire la liste noire téléphonique</string>
    <!-- [CHAR LIMIT=NONE] Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_readPhoneBlacklist">Permet a une application de lire des informations sur les numéros de téléphone qui sont bloqués pour les appels ou les messages entrants.</string>

    <!-- [CHAR LIMIT=NONE] Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_changePhoneBlacklist">Changer la liste noire du téléphone</string>
    <!-- [CHAR LIMIT=NONE] Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_changePhoneBlacklist">Permet à une application de modifier les numéros de téléphone qui sont bloqués pour les appels ou les messages entrants.</string>

    <!-- Privacy Guard -->
    <string name="permlab_changePrivacyGuardState">Activer ou désactiver privacy guard</string>
    <string name="permdesc_changePrivacyGuardState">Autorise l\'application à déterminer si une autre application est lancée avec privacy Guard. Lorsqu\'une application est lancée avec Privacy Guard, elle n\'aura pas accès aux données personnelles comme les contacts, le journal d\'appels, ou les messages.</string>
    <string name="privacy_guard_notification">Privacy Guard activé</string>
    <string name="privacy_guard_notification_detail"><xliff:g id="app">%1$s</xliff:g> ne pourra pas accéder à vos données personnelles.</string>
    <string name="privacy_guard_custom_notification_detail"><xliff:g id="app">%1$s</xliff:g> ne pourra pas accéder aux permissions qui lui sont refusées.</string>

    <!-- Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_accessTorchService">service d\'accès de la torche</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_accessTorchService">Permet à une application d\'accéder au service de la torche. Ne devrait jamais être nécessaire pour les applications normales.</string>

    <!-- Title of an application permission, listed so the user can choose whether they want the application to do this. -->
    <string name="permlab_setKeyguardWallpaper">Change le fond d\'écran du verrouillage</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_setKeyguardWallpaper">Permet à une application de modifier le fond du verrouillage de l\'écran.</string>

    <!-- Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_recordScreen">Enregistre le contenu en cous d\'affichage</string>

    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_recordScreen">Permet à l\'application d\'enregistrer l\'écran des utilisateurs</string>
	
    <!-- label for screen record item in phone options dialog -->
    <string name="global_action_screen_record">Enregistrement d\'écran</string>
    
    <string name="silent_mode_priority">Prioritaire</string>
    <string name="silent_mode_none">Aucune</string>
    
    <!-- Theme installation error notification -->
    <string name="theme_install_error_title">Échec lors de l\'installation du thème</string>
    <string name="theme_install_error_message">%s n\'a pas pu être installé</string>
    
    <!-- Theme reset notification -->
    <string name="theme_reset_notification_title">Réinitialisation du thème</string>
    <string name="theme_reset_notification_body">Le thème système est restauré en raison de multiples plantages d\'applications</string>

    <!-- Notification which notifies user flashlight is enabled -->
    <string name="quick_settings_tile_flashlight_not_title">Lampe de poche allumée</string>
    <string name="quick_settings_tile_flashlight_not_summary">Appuyer pour éteindre</string>
</resources>
